{
  "name": "@cristobalgvera/nestjs-environment",
  "version": "0.1.3",
  "author": "Crist√≥bal Gajardo Vera <cristobalgajardo.v@gmail.com>",
  "description": "Environment module configuration library that allows you to easily setup and validate your environment with minimal configuration.",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "release": "pnpm run prebuild && pnpm run build && changeset publish",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --check --ignore-unknown --write",
    "format:all": "npm run format -- .",
    "lint": "eslint --fix",
    "lint:all": "npm run lint -- \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@automock/jest": "^1.0.1",
    "@changesets/cli": "^2.26.1",
    "@golevelup/ts-jest": "^0.3.7",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "joi": "^17.9.2",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "nestjs",
    "environment"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristobalgvera/nestjs-environment"
  },
  "homepage": "https://github.com/cristobalgvera/nestjs-environment"
}
